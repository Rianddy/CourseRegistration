<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.netbeans.org/schema/semesterXmlSchema"
    xmlns:tns="http://xml.netbeans.org/schema/semesterXmlSchema"
    elementFormDefault="qualified">
    <xsd:complexType name="semester">
        <xsd:sequence>
            <xsd:element name="semid" type="xsd:string"></xsd:element>
            <xsd:element name="semname" type="xsd:string"></xsd:element>
            <xsd:element name="startdate" type="xsd:date"></xsd:element>
            <xsd:element name="enddate" type="xsd:date"></xsd:element>
            <xsd:element name="adddropdate" type="xsd:date"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
