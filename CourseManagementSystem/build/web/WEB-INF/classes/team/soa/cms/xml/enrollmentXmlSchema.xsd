<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.netbeans.org/schema/enrollmentXmlSchema"
    xmlns:tns="http://xml.netbeans.org/schema/enrollmentXmlSchema"
    elementFormDefault="qualified">
    <xsd:complexType name="enrollment">
        <xsd:sequence>
            <xsd:element name="stuenrollid" type="xsd:string"></xsd:element>
            <xsd:element name="stuid" type="xsd:string"></xsd:element>
            <xsd:element name="classid" type="xsd:string"></xsd:element>
            <xsd:element name="courseid" type="xsd:string"></xsd:element>
            <xsd:element name="grade" type="xsd:string"></xsd:element>
            <xsd:element name="score" type="xsd:float"></xsd:element>
            <xsd:element name="status" type="xsd:string"></xsd:element>
            <xsd:element name="stuenrolltime" type="xsd:dateTime"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
