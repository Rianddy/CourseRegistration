<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.netbeans.org/schema/stuclassppermresultXmlSchema"
    xmlns:tns="http://xml.netbeans.org/schema/stuclassppermresultXmlSchema"
    elementFormDefault="qualified">
    <xsd:complexType name="permissionresult">
        <xsd:sequence>
            <xsd:element name="studentinfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="stuid" type="xsd:string"></xsd:element>
                        <xsd:element name="stuname" type="xsd:string"></xsd:element>
                        <xsd:element name="stuemail" type="xsd:string"></xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="classinfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="classid" type="xsd:string"></xsd:element>
                        <xsd:element name="courseid" type="xsd:string"></xsd:element>
                        <xsd:element name="coursename" type="xsd:string"></xsd:element>
                        <xsd:element name="opensemester" type="xsd:string"></xsd:element>
                        <xsd:element name="regresult" type="xsd:string"></xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
            <xsd:element name="facultyinfo">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="facid" type="xsd:string"></xsd:element>
                        <xsd:element name="facname" type="xsd:string"></xsd:element>
                        <xsd:element name="facemail" type="xsd:string"></xsd:element>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
