<?xml version="1.0" encoding="UTF-8"?>

<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            targetNamespace="http://xml.netbeans.org/schema/facultyXmlSchema"
    xmlns:tns="http://xml.netbeans.org/schema/facultyXmlSchema"
    elementFormDefault="qualified">
    <xsd:complexType name="faculty">
        <xsd:sequence>
            <xsd:element name="facid" type="xsd:string"></xsd:element>
            <xsd:element name="facname" type="xsd:string"></xsd:element>
            <xsd:element name="facstatus" type="xsd:string"></xsd:element>
            <xsd:element name="facemail" type="xsd:string"></xsd:element>
            <xsd:element name="faccell" type="xsd:string"></xsd:element>
            <xsd:element name="facdepart" type="xsd:string"></xsd:element>
        </xsd:sequence>
    </xsd:complexType>
</xsd:schema>
